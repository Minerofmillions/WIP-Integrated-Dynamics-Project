{
  "vars": {
    "bTrue": {
      "type": "Boolean",
      "value": "true"
    },
    "bFalse": {
      "type": "Boolean",
      "value": "false"
    }
  },
  "tests": {
    "GENERAL_CHOICE": {
      "testLogicalChoice": [
        {
          "args": ["bTrue", "i1", "i2"]
        },
        {
          "args": ["bFalse", "i1", "i2"]
        }
      ],
      "testInvalidInputSizeChoiceLarge": [
        {
          "args": ["bTrue", "bTrue", "bTrue", "bTrue"]
        }
      ],
      "testInvalidInputSizeChoiceSmall": [
        {
          "args": ["bTrue", "bTrue"]
        }
      ],
      "testInvalidInputTypeChoice": [
        {
          "args": ["DUMMY_VARIABLE", "DUMMY_VARIABLE", "DUMMY_VARIABLE"]
        }
      ],
      "testInvalidLogicalChoiceDifferentTypes": [
        {
          "args": ["bFalse", "i1", "DUMMY_VARIABLE"]
        }
      ]
    },
    "GENERAL_IDENTITY": {
      "testLogicalIdentity": [
        {
          "args": ["i1"]
        },
        {
          "args": ["bFalse"]
        }
      ],
      "testInvalidInputSizeIdentityLarge": [
        {
          "args": ["bTrue", "bTrue"]
        }
      ],
      "testInvalidInputSizeIdentitySmall": [
        {
          "args": []
        }
      ]
    },
    "GENERAL_CONSTANT": {
      "testLogicalConstant": [
        {
          "args": ["i1", "bFalse"]
        },
        {
          "args": ["bFalse", "i1"]
        }
      ],
      "testInvalidInputSizeConstantLarge": [
        {
          "args": ["bTrue", "bTrue", "bTrue"]
        }
      ],
      "testInvalidInputSizeConstantSmall": [
        {
          "args": ["bTrue"]
        }
      ]
    },
    "NULLABLE_ISNULL": {
      "testLogicalIsNull": [
        {
          "args": ["i1"]
        },
        {
          "args": ["bFalse"]
        }
      ],
      "testInvalidInputSizeIsNullLarge": [
        {
          "args": ["bTrue", "bTrue"]
        }
      ],
      "testInvalidInputSizeIsNullSmall": [
        {
          "args": []
        }
      ]
    },
    "NULLABLE_ISNOTNULL": {
      "testLogicalIsNotNull": [
        {
          "args": ["i1"]
        },
        {
          "args": ["bFalse"]
        }
      ],
      "testInvalidInputSizeIsNotNullLarge": [
        {
          "args": ["bTrue", "bTrue"]
        }
      ],
      "testInvalidInputSizeIsNotNullSmall": [
        {
          "args": []
        }
      ]
    }
  }
}
